{
  "name": "magic-box",
  "version": "1.0.0",
  "description": "electron box",
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "compile": "gulp compile",
    "compile:watch": "gulp compile && gulp compile:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lloyd-lin/magic-box.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "lloyd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lloyd-lin/magic-box/issues"
  },
  "homepage": "https://github.com/lloyd-lin/magic-box#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/standalone": "7.4.5",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.1.19",
    "babel-loader": "8.0.6",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "check-for-leaks": "^1.2.0",
    "classnames": "2.2.6",
    "del": "^5.0.0",
    "devtron": "^1.3.0",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.2.0",
    "gulp": "^4.0.1",
    "gulp-babel": "8.0.0",
    "gulp-clean": "0.4.0",
    "gulp-concat": "2.6.1",
    "gulp-sass": "4.0.2",
    "gulp-sequence": "1.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-typescript": "^5.0.1",
    "husky": "^0.14.3",
    "merge2": "^1.2.4",
    "mini-css-extract-plugin": "0.7.0",
    "mocha": "^5.2.0",
    "node-easy-cert": "^1.3.1",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.0.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-electron-web-view": "^2.0.1",
    "react-hot-loader": "4.11.1",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "sass-loader": "7.1.0",
    "signcode": "^0.5.0",
    "spectron": "^5.0.0",
    "standard": "^8.2.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "through2": "3.0.1",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "antd": "^3.23.4",
    "anyproxy": "^4.1.0",
    "electron": "^4.0.1",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "glob": "^7.1.0",
    "highlight.js": "^9.3.0",
    "mobx": "^5.13.1",
    "styled-components": "^4.4.0",
    "typescript": "^3.6.2",
    "update-electron-app": "^1.1.1"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
